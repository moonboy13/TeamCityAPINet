<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>TeamCity.NETAPI</PackageId>
    <Version>0.1.7</Version>
    <Authors>Kyle Conlon</Authors>
    <Product>TeamCity API .NET Wrapper</Product>
    <Description>Simple .NET wrapper for the TeamCity REST API</Description>
    <RepositoryUrl>https://github.com/moonboy13/TeamCityAPINet</RepositoryUrl>
    <PackageProjectUrl>https://github.com/moonboy13/TeamCityAPINet/blob/master/LICENSE</PackageProjectUrl>
    <PackageReleaseNotes>Initial package release for internal testing</PackageReleaseNotes>
    <PackageTags>TeamCity .NET</PackageTags>
    <AssemblyVersion>0.1.7.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="Unity" Version="5.10.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Generated Files\" />
  </ItemGroup>

</Project>
